{
  "name": "react-redux-starter-kit",
  "version": "3.0.0-alpha.0",
  "description": "Get started with React, Redux, and React-Router!",
  "main": "index.js",
  "engines": {
    "node": ">=4.2.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "lint": "eslint ./",
    "lint:fix": "npm run lint -- --fix",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "dev:no-debug": "npm run dev -- --no_debug",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod",
    "test-ui": "protractor tests-ui/conf.babel.js",
    "test-unitserver": "mocha --harmony --reporter spec --compilers js:babel-register --compilers ls:livescript tests-unitserver",
    "test-integration": "mocha --harmony --reporter spec --compilers js:babel-register --compilers ls:livescript tests-integration",
    "test-unitclient": "babel-node ./node_modules/karma/bin/karma start build/karma.conf",
    "codecov": "cat coverage/*/lcov.info | codecov"
  },
  "betterScripts": {
    "compile": {
      "command": "babel-node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon --exec babel-node bin/server",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "./vault/decrypt.sh && babel-node bin/server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "npm run test-unitserver && npm run test-integration && npm run test-unitclient && npm run lint",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davezuko/react-redux-starter-kit.git"
  },
  "author": "Igor Fischer",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.3.17",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "better-npm-run": "0.1.0",
    "bluebird": "^3.3.5",
    "body-parser": "^1.15.1",
    "bootstrap": "^3.3.6",
    "cssnano": "^3.3.2",
    "debug": "^3.1.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "fs-extra": "^4.0.3",
    "github": "^13.0.1",
    "history": "^3.3.0",
    "html-webpack-plugin": "^2.7.1",
    "imports-loader": "^0.7.1",
    "jquery": "^3.2.1",
    "json-loader": "^0.5.4",
    "koa": "^2.0.0-alpha.3",
    "koa-connect-history-api-fallback": "^0.3.0",
    "koa-convert": "^1.2.0",
    "koa-proxy": "^0.9.0",
    "koa-route": "^3.2.0",
    "koa-static": "^4.0.2",
    "leveldown": "^2.1.1",
    "levelup": "^2.0.1",
    "livescript": "^1.5.0",
    "lodash": "4.17.4",
    "mailgun-js": "^0.13.1",
    "moment": "^2.13.0",
    "node-uuid": "^1.4.7",
    "normalize.css": "^7.0.0",
    "postcss-loader": "^2.0.9",
    "promise-polyfill": "^6.1.0",
    "react": "^15.5.0",
    "react-dom": "^15.5.0",
    "react-redux": "^5.0.6",
    "react-router": "^3.2.0",
    "react-router-redux": "^4.0.8",
    "redux": "^3.0.0",
    "redux-thunk": "^2.0.0",
    "rimraf": "^2.5.1",
